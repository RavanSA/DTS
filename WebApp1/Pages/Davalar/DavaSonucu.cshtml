@page
@model WebApp1.Pages.Davalar.DavaSonucuModel
@{
}



<div class="border container" style="padding:30px;">
    <form method="post">

        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.DavaKayitNo"></label>
            </div>
            <div class="col-6">
                @{
                    var id = Request.QueryString.ToString();
                    string[] tokens = id.Split("=");
                }
                @tokens[1]
                <input asp-for="DavaSonucu.DavaKayitNo" value="@tokens[1].ToString()" type="hidden" class="form-control" />

            </div>
        </div>

        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.SonucTarihi"></label>
            </div>
            <div class="col-6">
                <input asp-for="DavaSonucu.SonucTarihi" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.KaldirmaNo"></label>
            </div>
            <div class="col-6">
                <input asp-for="DavaSonucu.KaldirmaNo" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.DavaSonucu1"></label>
            </div>
            <div class="col-6">
                <input asp-for="DavaSonucu.DavaSonucu1" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.IcraSafhasi"></label>
            </div>
            <div class="col-6">
                <input asp-for="DavaSonucu.IcraSafhasi" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.AsilAlacak"></label>
            </div>
            <div class="col-6">
                <input asp-for="DavaSonucu.AsilAlacak" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.IslenmisFaiz"></label>
            </div>
            <div class="col-6">
                <input asp-for="DavaSonucu.IslenmisFaiz" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.MahkemeHarcveMasrafi"></label>
            </div>
            <div class="col-6">
                <input asp-for="DavaSonucu.MahkemeHarcveMasrafi" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.IcraMasraflari"></label>
            </div>
             <div class="col-6">
                <input asp-for="DavaSonucu.IcraMasraflari" class="form-control" />
            </div>
        </div>


        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.VekaletUcretleri"></label>
            </div>
            <div class="col-6">
                <input asp-for="DavaSonucu.VekaletUcretleri" class="form-control" />
            </div>
        </div>


        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.IcraVekaletUcreti"></label>
            </div>
            <div class="col-6">
                <input asp-for="DavaSonucu.IcraVekaletUcreti" class="form-control" />
            </div>
        </div>


        <div class="form-group row">
            <div class="col-4">
                <label asp-for="DavaSonucu.OdemeTarihi"></label>
            </div>
            <div class="col-6">
                <input asp-for="DavaSonucu.OdemeTarihi" class="form-control" />
            </div>
        </div>

        <div class="form-group row offset-4">
            <div class="col-3">
                <input type="submit" value="Güncelle" class="btn btn-primary form-control" />
            </div>
            <div class="col-3">
                <a asp-page="Index" class="btn btn-success form-control">Geri dön</a>
            </div>
        </div>

    </form>
</div>
